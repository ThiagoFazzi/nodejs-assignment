version: '3'
services:
    client-app:
        container_name: client-app
        build:
            context: .
            dockerfile: Dockerfile
        volumes:
        - '.:/usr/src/app'
        - '/usr/src/app/node_modules'
        ports:
        - '3000:3000'
        environment:
        - NODE_ENV=development
        - MAPS_KEY=YOUR_GOOGLE_KEY